{
  "name": "aries-agent-electron",
  "version": "0.1.0",
  "private": true,
  "description": "Aries Agent Electron with aries-framework-javascript",
  "author": "Berend Sliedrecht",
  "build": {
    "productName": "Aries Agent",
    "appId": "com.blueberi.ariesagentelectron",
    "mac": {
      "icon": "./public/assets/icon.icns"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "aries-framework": "./aries-framework-attachment-patch.tgz",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.2",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "indy-sdk": "^1.16.0-dev-1633",
    "node-fetch": "^2.6.1",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "rxjs": "^6.6.7",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/indy-sdk": "^1.15.2",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.5.10",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.0.2",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "electron-rebuild": "^2.3.5",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.0",
    "tslog": "^3.2.0",
    "wait-on": "^5.3.0"
  }
}
